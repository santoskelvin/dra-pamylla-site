/* Marquee (faixa deslizante) */
.marquee-container {
    width: 100%;
    overflow: hidden;
    background-color: rgba(203, 161, 53, 0.85);
    border-top: 2px solid rgba(255, 255, 255, 0.3);
    border-bottom: 2px solid rgba(255, 255, 255, 0.3);
    padding: 15px 0;
    position: relative;
    margin-top: auto;
    z-index: 10;
    box-shadow: 0 -5px 15px rgba(0, 0, 0, 0.1);
    backdrop-filter: blur(5px);
    -webkit-backdrop-filter: blur(5px);
}

.marquee-content {
    display: flex;
    align-items: center;
    white-space: nowrap;
    animation: marquee 30s linear infinite;
}

.marquee-content:hover {
    animation-play-state: paused;
}

.marquee-item {
    display: flex;
    align-items: center;
    font-size: 1.1rem;
    color: #fff;
    font-weight: 500;
    padding: 0 25px;
    transition: transform 0.3s ease, opacity 0.3s ease;
}

.marquee-item:hover {
    transform: scale(1.05);
    opacity: 0.9;
}

.marquee-item .star-icon {
    color: #fff;
    font-size: 0.9rem;
    margin-right: 10px;
    animation: pulse 2s infinite;
}

@keyframes pulse {
    0% {
        transform: scale(1);
    }
    50% {
        transform: scale(1.2);
    }
    100% {
        transform: scale(1);
    }
}

@keyframes marquee {
    0% {
        transform: translateX(0);
    }
    100% {
        transform: translateX(-50%);
    }
}

/* Media queries para o marquee */
@media (max-width: 992px) {
    .marquee-container {
        margin-top: -10px;
        position: relative;
        bottom: 0;
        z-index: 5;
    }
    
    .marquee-item {
        font-size: 1rem;
        padding: 0 20px;
    }
}

@media (max-width: 768px) {
