}

@keyframes blurText {
    0% { filter: blur(5px); opacity: 0; }
    100% { filter: blur(0); opacity: 1; }
}

/* Animações especiais para a seção Hero */
.hero-animation {
    opacity: 0;
    visibility: hidden;
    transform: translateY(30px) scale(0.95);
    transition: opacity 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275), 
