}

.typewriter-container {
    opacity: 0;
    visibility: hidden;
}

.typewriter-container.revealed {
    opacity: 1;
    visibility: visible;
}

.typewriter-container.revealed .typewriter-text {
    display: inline-block;
    overflow: hidden;
    white-space: nowrap;
    animation: typewriter 1.5s steps(40, end) forwards, blinkCursor 0.5s step-end infinite;
    position: relative;
}

.typewriter-container.revealed .typewriter-text::after {
    content: '|';
