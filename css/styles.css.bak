/* Header Scroll Effect */
header.scrolled {
    padding: 40px 5px;
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
    background-color: rgba(245, 240, 235, 1); /* Full opacity on scroll */
}

/* Animações e Efeitos de Carregamento */
body {
    opacity: 0;
    transition: opacity 0.5s ease;
}

body.page-loaded {
    opacity: 1;
}

/* Spinner de carregamento */
.page-loader {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #f5f0eb;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 9999;
    transition: opacity 0.5s ease, visibility 0.5s ease;
}

.page-loaded .page-loader {
    opacity: 0;
    visibility: hidden;
}

.spinner {
    width: 50px;
    height: 50px;
    border: 4px solid rgba(203, 161, 53, 0.2);
    border-radius: 50%;
    border-top-color: #CBA135;
    animation: spin 1s linear infinite;
}

@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

/* Efeitos de animação de elementos */
.fade-in {
    opacity: 0;
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.slide-in-left {
    opacity: 0;
    transform: translateX(-30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.slide-in-right {
    opacity: 0;
    transform: translateX(30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.slide-in-up {
    opacity: 0;
    transform: translateY(30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.scale-in {
    opacity: 0;
    transform: scale(0.9);
    transition: opacity 0.8s ease, transform 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}

/* Classes para quando os elementos são animados */
.animated {
    opacity: 1 !important;
    transform: translate(0) scale(1) !important;
}

/* Certando-se que elementos revelados sejam visíveis */
.revealed, 
.reveal.revealed,
.reveal-img.revealed {
    opacity: 1 !important;
    transform: translateY(0) !important;
    visibility: visible !important;
}

/* Garantindo que elementos com animação sejam visíveis por padrão */
body.page-loaded .hero-content,
body.page-loaded .marquee-container,
body.page-loaded .section-header,
body.page-loaded .sobre-content,
body.page-loaded .sobre-image,
body.page-loaded .sobre-texto,
body.page-loaded .carousel-container,
body.page-loaded .testimonials,
body.page-loaded .testimonial,
body.page-loaded .beneficios-grid,
body.page-loaded .beneficio-card,
body.page-loaded .lentes-content,
body.page-loaded .lentes-info,
body.page-loaded .lentes-image,
body.page-loaded .servicos-grid,
body.page-loaded .servico-card,
body.page-loaded .cta-box,
body.page-loaded .faq-container,
body.page-loaded .faq-item,
body.page-loaded .footer-content,
body.page-loaded .footer-info,
body.page-loaded .footer-contact,
body.page-loaded .map-container,
body.page-loaded .copyright {
    transition-delay: 0s !important;
    visibility: visible !important;
}

/* Configurações específicas para diferentes seções */
.section-header {
    opacity: 0;
    transform: translateY(20px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.section-header.animated {
    opacity: 1;
    transform: translateY(0);
}

/* Cards com efeito de stagger */
.beneficio-card, .servico-card, .faq-item {
    opacity: 0;
    transform: translateY(30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.beneficio-card.animated:nth-child(1) { transition-delay: 0.1s; }
.beneficio-card.animated:nth-child(2) { transition-delay: 0.2s; }
.beneficio-card.animated:nth-child(3) { transition-delay: 0.3s; }
.beneficio-card.animated:nth-child(4) { transition-delay: 0.4s; }
.beneficio-card.animated:nth-child(5) { transition-delay: 0.5s; }
.beneficio-card.animated:nth-child(6) { transition-delay: 0.6s; }

.servico-card.animated:nth-child(1) { transition-delay: 0.1s; }
.servico-card.animated:nth-child(2) { transition-delay: 0.2s; }
.servico-card.animated:nth-child(3) { transition-delay: 0.3s; }
.servico-card.animated:nth-child(4) { transition-delay: 0.4s; }

.faq-item.animated:nth-child(1) { transition-delay: 0.1s; }
.faq-item.animated:nth-child(2) { transition-delay: 0.2s; }
.faq-item.animated:nth-child(3) { transition-delay: 0.3s; }
.faq-item.animated:nth-child(4) { transition-delay: 0.4s; }
.faq-item.animated:nth-child(5) { transition-delay: 0.5s; }
.faq-item.animated:nth-child(6) { transition-delay: 0.6s; }

/* Animações para elementos flutuantes */
@keyframes bounce {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

@keyframes float {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-15px); }
}

@keyframes pulse {
    0%, 100% { transform: scale(1); }
    50% { transform: scale(1.05); }
}

.animate-bounce {
    animation: bounce 3s ease-in-out infinite;
}

.animate-float {
    animation: float 6s ease-in-out infinite;
}

.animate-pulse {
    animation: pulse 4s ease-in-out infinite;
}

/* Efeito de hover nos cards com animação */
.beneficio-card:hover, .servico-card:hover {
    transform: translateY(-10px);
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
}

.testimonial {
    transition: transform 0.4s ease, box-shadow 0.4s ease;
}

.testimonial:hover {
    transform: translateY(-5px);
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
}

/* Animações */
.reveal {
    opacity: 0;
    transform: translateY(50px);
    transition: opacity 0.6s ease-out, transform 0.6s ease-out;
}

.reveal.revealed {
    opacity: 1;
    transform: translateY(0);
}

/* Delay para elementos em sequência (opcional) */
.beneficio-card.reveal:nth-child(2) { transition-delay: 0.1s; }
.beneficio-card.reveal:nth-child(3) { transition-delay: 0.2s; }
.beneficio-card.reveal:nth-child(4) { transition-delay: 0.3s; }
.beneficio-card.reveal:nth-child(5) { transition-delay: 0.4s; }
.beneficio-card.reveal:nth-child(6) { transition-delay: 0.5s; }

.servico-card.reveal:nth-child(2) { transition-delay: 0.1s; }
.servico-card.reveal:nth-child(3) { transition-delay: 0.2s; }
.servico-card.reveal:nth-child(4) { transition-delay: 0.3s; }

.faq-item.reveal:nth-child(2) { transition-delay: 0.1s; }
.faq-item.reveal:nth-child(3) { transition-delay: 0.2s; }
.faq-item.reveal:nth-child(4) { transition-delay: 0.3s; }
.faq-item.reveal:nth-child(5) { transition-delay: 0.4s; }
.faq-item.reveal:nth-child(6) { transition-delay: 0.5s; }

