/* Efeito de texto sendo digitado (typewriter) - Versão aprimorada */
.typewriter {
    opacity: 0;
    visibility: hidden;
    position: relative;
    overflow: hidden;
    display: inline-block;
    width: 100%;
    color: inherit;
    transition: opacity 0.5s ease;
}

.typewriter.revealed {
    opacity: 1;
    visibility: visible;
}

.typewriter.typewriter-complete::after {
    content: '';
    display: none;
}

.typewriter::after {
    content: '|';
    position: relative;
    display: inline-block;
    color: inherit;
    opacity: 1;
    animation: blink 0.7s infinite;
    margin-left: 2px;
}

/* Efeito de blur para revelar textos */
.blur-reveal {
    opacity: 0;
    filter: blur(8px);
    transform: translateY(10px) scale(0.95);
    transition: all 0.8s cubic-bezier(0.215, 0.61, 0.355, 1);
}

.blur-reveal.revealed {
    opacity: 1;
    filter: blur(0);
    transform: translateY(0) scale(1);
}

/* Progressive reveal para elementos em sequência */
.progressive-reveal {
    opacity: 0;
    transform: translateY(30px);
    transition: all 0.6s cubic-bezier(0.165, 0.84, 0.44, 1);
}

.progressive-reveal.revealed {
    opacity: 1;
    transform: translateY(0);
}

/* Containers para animações em sequência */
.sequential-container .sequential-item {
    opacity: 0;
    transform: translateY(20px);
    transition: all 0.5s ease;
}

.sequential-container.revealed .sequential-item {
    opacity: 1;
    transform: translateY(0);
}

.sequential-container.revealed .sequential-item:nth-child(1) { transition-delay: 0.1s; }
.sequential-container.revealed .sequential-item:nth-child(2) { transition-delay: 0.2s; }
.sequential-container.revealed .sequential-item:nth-child(3) { transition-delay: 0.3s; }
.sequential-container.revealed .sequential-item:nth-child(4) { transition-delay: 0.4s; }
.sequential-container.revealed .sequential-item:nth-child(5) { transition-delay: 0.5s; }
.sequential-container.revealed .sequential-item:nth-child(6) { transition-delay: 0.6s; }

/* Efeito de revelação suave para textos */
.text-reveal {
    display: inline-block;
    position: relative;
    overflow: hidden;
}

.text-reveal span {
    display: inline-block;
    transform: translateY(100%);
    opacity: 0;
    transition: transform 0.6s cubic-bezier(0.19, 1, 0.22, 1), 
                opacity 0.6s cubic-bezier(0.19, 1, 0.22, 1);
}

.text-reveal.revealed span {
    transform: translateY(0);
    opacity: 1;
}

/* Efeito de fade-in com stagger para elementos em grade */
.fade-grid .grid-item {
    opacity: 0;
    transform: translateY(30px);
    transition: all 0.8s cubic-bezier(0.215, 0.61, 0.355, 1);
}

.fade-grid.revealed .grid-item {
    opacity: 1;
    transform: translateY(0);
}

.fade-grid.revealed .grid-item:nth-child(1) { transition-delay: 0.1s; }
.fade-grid.revealed .grid-item:nth-child(2) { transition-delay: 0.2s; }
.fade-grid.revealed .grid-item:nth-child(3) { transition-delay: 0.3s; }
.fade-grid.revealed .grid-item:nth-child(4) { transition-delay: 0.4s; }
.fade-grid.revealed .grid-item:nth-child(5) { transition-delay: 0.5s; }
.fade-grid.revealed .grid-item:nth-child(6) { transition-delay: 0.6s; }
.fade-grid.revealed .grid-item:nth-child(7) { transition-delay: 0.7s; }
.fade-grid.revealed .grid-item:nth-child(8) { transition-delay: 0.8s; }

/* Efeito de animação de foco para elementos importantes */
.focus-animate {
    transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}

.focus-animate:hover {
    transform: scale(1.05);
    box-shadow: 0 10px 25px rgba(0, 0, 0, 0.1);
    z-index: 5;
}

/* Efeito de scale para imagens com gradiente sobreposto */
.scale-reveal {
    opacity: 0;
    transform: scale(0.85);
    transition: all 0.7s cubic-bezier(0.25, 0.46, 0.45, 0.94);
    position: relative;
    overflow: hidden;
}

.scale-reveal::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(to bottom, rgba(0,0,0,0) 0%, rgba(0,0,0,0.2) 100%);
    opacity: 0;
    transition: opacity 0.6s ease;
}

.scale-reveal.revealed {
    opacity: 1;
    transform: scale(1);
}

.scale-reveal.revealed::after {
    opacity: 1;
}

/* Efeito de fade staggered para parágrafos dentro de seções */
.fade-paragraphs p {
    opacity: 0;
    transform: translateY(20px);
    transition: all 0.6s ease;
}

.fade-paragraphs.revealed p {
    opacity: 1;
    transform: translateY(0);
}

.fade-paragraphs.revealed p:nth-child(1) { transition-delay: 0.1s; }
.fade-paragraphs.revealed p:nth-child(2) { transition-delay: 0.2s; }
.fade-paragraphs.revealed p:nth-child(3) { transition-delay: 0.3s; }
.fade-paragraphs.revealed p:nth-child(4) { transition-delay: 0.4s; }
.fade-paragraphs.revealed p:nth-child(5) { transition-delay: 0.5s; }

/* Melhorando o sistema de segurança para garantir visibilidade */
.force-visible {
    opacity: 1 !important;
    visibility: visible !important;
    display: block !important;
    transform: none !important; 
    filter: none !important;
    transition: none !important;
}

.force-visible.d-flex,
.force-visible.display-flex,
.force-visible[style*="display: flex"] {
    display: flex !important;
}

.force-visible.d-grid,
.force-visible.display-grid,
.force-visible[style*="display: grid"] {
    display: grid !important;
}

/* Certando-se que elementos revelados sejam visíveis */
.revealed, 
.reveal.revealed,
.reveal-img.revealed,
.typewriter.revealed,
.typewriter-container.revealed,
.blur-reveal.revealed,
.progressive-reveal.revealed,
.sequential-container.revealed,
.text-reveal.revealed,
.fade-grid.revealed,
.scale-reveal.revealed,
.fade-paragraphs.revealed {
    opacity: 1 !important;
    transform: translateY(0) !important;
    visibility: visible !important;
    filter: none !important;
}

/* Ajustes responsivos para as animações */
@media (max-width: 768px) {
    /* Reduzir a complexidade das animações em dispositivos móveis */
    .typewriter::after {
        animation-duration: 0.5s;
    }
    
    .sequential-container.revealed .sequential-item:nth-child(n) {
        transition-delay: 0.1s;
    }
    
    .fade-grid.revealed .grid-item:nth-child(n) {
        transition-delay: 0.1s;
    }
}

/* Animações específicas para elementos que devem aparecer na ordem */
.scroll-sequence .sequence-item {
    opacity: 0;
    transform: translateY(30px);
    filter: blur(5px);
    transition: all 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
}

.scroll-sequence.revealed .sequence-item {
    opacity: 1;
    transform: translateY(0);
    filter: blur(0);
}

.scroll-sequence.revealed .sequence-item:nth-child(1) { transition-delay: 0.1s; }
.scroll-sequence.revealed .sequence-item:nth-child(2) { transition-delay: 0.2s; }
.scroll-sequence.revealed .sequence-item:nth-child(3) { transition-delay: 0.3s; }
.scroll-sequence.revealed .sequence-item:nth-child(4) { transition-delay: 0.4s; }
.scroll-sequence.revealed .sequence-item:nth-child(5) { transition-delay: 0.5s; }
.scroll-sequence.revealed .sequence-item:nth-child(6) { transition-delay: 0.6s; }

/* Efeito de tipo máquina de escrever mais fluido */
.typewriter-text {
    display: inline-block;
    overflow: hidden;
    border-right: 2px solid;
    white-space: nowrap;
    margin: 0 auto;
    animation: 
        typing 3.5s steps(40, end),
        blink .75s step-end infinite;
    opacity: 0;
}

.typewriter-text.revealed {
    opacity: 1;
}

/* Prevenção para dispositivos com modo de economia de bateria */
@media (prefers-reduced-motion: reduce) {
    * {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
        scroll-behavior: auto !important;
    }
    
    .typewriter,
    .typewriter-text,
    .reveal,
    .reveal-img,
    .fade-in,
    .slide-in-left,
    .slide-in-right,
    .slide-in-up,
    .scale-in,
    .blur-reveal,
    .progressive-reveal,
    .sequential-container .sequential-item,
    .text-reveal span,
    .fade-grid .grid-item,
    .scale-reveal,
    .fade-paragraphs p,
    .scroll-sequence .sequence-item {
        opacity: 1 !important;
        transform: none !important;
        filter: none !important;
        animation: none !important;
        transition: none !important;
    }
}

