    align-items: flex-start;
    gap: 10px;
}

.lentes-beneficios li i {
    color: #CBA135;
    font-size: 1.1rem;
    margin-top: 5px;
}

/* Outros Serviços */
.servicos-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 2rem;
}

.servico-card {
    background-color: rgba(245, 240, 235, 0.2);
    border-radius: 10px;
    overflow: hidden;
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
    transition: all 0.3s ease;
    opacity: 0;
    transform: translateY(30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.servico-card:hover {
    transform: translateY(-10px);
}

.servico-card img {
    width: 100%;
    height: 200px;
    object-fit: cover;
}

.servico-card h3 {
    padding: 1.5rem 1.5rem 0.5rem 1.5rem;
    font-size: 1.2rem;
}

.servico-card p {
    padding: 0 1.5rem 1.5rem 1.5rem;
    opacity: 0.9;
}

/* CTA Final */
.cta-box {
    background: linear-gradient(45deg, #3A2F2B, #7C736A);
    border-radius: 15px;
    padding: 3rem;
    text-align: center;
    color: white;
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.2);
}

.cta-box h2 {
    font-size: 2.2rem;
    margin-bottom: 1rem;
}

.cta-box p {
    margin-bottom: 2rem;
    font-size: 1.1rem;
}

/* FAQ */
.faq-container {
    max-width: 800px;
    margin: 0 auto;
}

.faq-item {
    border-bottom: 1px solid rgba(245, 240, 235, 0.3);
    margin-bottom: 1rem;
    opacity: 0;
    transform: translateY(30px);
    transition: opacity 0.8s ease, transform 0.8s ease;
}

.faq-question {
    display: flex;
    justify-content: space-between;
    align-items: center;
    cursor: pointer;
    padding: 1rem 0;
}

.faq-question h3 {
    margin: 0;
    font-size: 1.1rem;
}

.faq-answer {
    height: 0;
    overflow: hidden;
    transition: all 0.3s ease;
    opacity: 0;
}

.faq-item.active .faq-answer {
    height: auto;
    opacity: 1;
    padding-bottom: 1.5rem;
}

.faq-item.active .faq-question i {
    transform: rotate(180deg);
}

/* Footer */
footer {
    background-color: #3A2F2B;
    color: #F5F0EB;
    padding: 4rem 0 2rem 0;
}

.footer-content {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    gap: 2rem;
}

.footer-info {
    flex: 1;
    min-width: 300px;
}

.footer-logo {
    height: 80px;
    margin-bottom: 1.5rem;
}

.social-links {
    display: flex;
    gap: 1rem;
    margin-top: 1.5rem;
}

.social-icon {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    background-color: #7C736A;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 1.2rem;
    transition: all 0.3s ease;
}

.social-icon:hover {
    background-color: #CBA135;
    transform: translateY(-3px);
}

.footer-contact {
    flex: 2;
