}

/* Lentes */
.lentes-content {
    display: flex;
    align-items: center;
    gap: 3rem;
}

.lentes-info {
    flex: 1.5;
}

.lentes-image {
    flex: 1;
}

.lentes-image img {
    border-radius: 15px;
    box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);
}

.lentes-beneficios {
    margin-bottom: 2rem;
}

.lentes-beneficios li {
