#ajuda-dropdown .dropdown-item:nth-child(2) .icon-bg i {
    color: white;
}

#ajuda-dropdown .dropdown-item:nth-child(3) .icon-bg { /* FAQ */
    background-color: rgba(203, 161, 53, 0.15); /* Dourado claro do site */
}
#ajuda-dropdown .dropdown-item:nth-child(3) .icon-bg i {
    color: #CBA135; /* Dourado principal do site */
}

/* Ajuste para Mobile */
@media (max-width: 767px) {
  .ajuda-container {
    margin-right: 5px; /* Ajustar espaçamento */
  }
  .ajuda-btn {
    padding: 5px 10px;
    font-size: 0.9rem;
  }
  #ajuda-dropdown {
     min-width: 200px;
     /* Outros ajustes se necessário */
  }
}

/*
===========================================
  Seção Resultados - Aprimorada
===========================================
*/

#resultados {
  position: relative;
  overflow: hidden; /* Garante que nada transborde */
}

.carousel-container {
  position: relative;
  max-width: 900px;
  margin: 0 auto 4rem auto; /* Aumentar margem inferior */
  opacity: 0;
  transform: translateY(30px);
  transition: opacity 0.8s ease, transform 0.8s ease;
}

.carousel-container.revealed {
  opacity: 1;
  transform: translateY(0);
}

.carousel {
  display: flex;
  overflow: hidden; /* Essencial para o carrossel */
  border-radius: 15px;
  /* Adicionar perspectiva para animações 3D sutis se desejado */
  /* perspective: 1000px; */
  transition: transform 0.6s ease-in-out;
}

.carousel-item {
  min-width: 100%;
  flex: 1 0 100%;
  /* Animação mais sofisticada: slide + fade */
  transition: transform 0.7s cubic-bezier(0.68, -0.55, 0.27, 1.55), opacity 0.5s ease-in-out;
  backface-visibility: hidden;
  opacity: 1;
}

/* Animação sutil para o item ativo (opcional) */
.carousel-item.is-active {
  /* Exemplo: transform: scale(1.02); */
}

.antes-depois {
  display: grid; /* Usar grid para melhor responsividade */
  grid-template-columns: 1fr; /* Modificado para uma coluna, empilhando as imagens */
  gap: 1px; /* Pequeno espaço ou linha divisória */
  background-color: rgba(0, 0, 0, 0.05); /* Linha divisória */
  overflow: hidden; /* Garante que imagens não vazem */
}

.antes, .depois {
  position: relative;
  background-color: #fff; /* Fundo para caso imagem demore */
}

.antes img, .depois img {
  display: block;
  width: 100%;
  height: 400px; /* Altura fixa para consistência */
  object-fit: cover;
  transition: transform 0.5s ease;
}

.antes:hover img, .depois:hover img {
  transform: scale(1.05); /* Efeito de zoom no hover */
}

.tag {
  position: absolute;
  top: 15px;
  left: 15px;
  background-color: rgba(58, 47, 43, 0.85); /* Cor tema */
  color: white;
  padding: 6px 18px;
  border-radius: 20px;
  font-size: 0.85rem;
  font-weight: 500;
  backdrop-filter: blur(3px);
  z-index: 2;
}

.carousel-controls {
  position: absolute;
  top: 50%;
  /* Ajustar para ficar um pouco para dentro */
  left: 15px;
  right: 15px;
  display: flex;
  justify-content: space-between;
  transform: translateY(-50%);
  z-index: 10;
  pointer-events: none; 
}

.carousel-controls button {
  background-color: rgba(255, 255, 255, 0.9);
  border: 1px solid rgba(0, 0, 0, 0.08);
  width: 45px;
  height: 45px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: opacity 0.3s ease, transform 0.3s ease, background-color 0.3s ease, color 0.3s ease, box-shadow 0.3s ease; /* Adicionar transições faltando */
  color: #3A2F2B;
  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
  pointer-events: auto; /* Habilitar clique nos botões */
  opacity: 0.8; /* Mais visível por padrão */
  transform: scale(1); /* Tamanho normal por padrão */
}
